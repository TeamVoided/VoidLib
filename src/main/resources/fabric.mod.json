{
  "schemaVersion": 1,
  "id": "voidlib",
  "version": "${version}",
  "name": "Void Lib",
  "description": "A common library for Team Voided's projects",
  "authors": [
    "Team Voided"
  ],
  "contact": { },
  "license": "MIT",
  "icon": "assets/voidlib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "org.teamvoided.voidlib.VoidLibKt::onInitialize"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "org.teamvoided.voidlib.client.VoidLibClientKt::onInitialize"
      }
    ]
  },
  "mixins": [
    "attachments.mixins.json",
    "pow.mixins.json",
    "dimutils.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.4+kotlin.1.8.21",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": { }
}
